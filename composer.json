{
    "name": "hemend/php-library",
    "description": "Hemend libraries",
    "keywords" : [
        "hemend",
        "library"
    ],
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hemend\\Library\\": "src/"
        }
    },
    "authors": [
        {
            "name": "balal",
            "email": "arastbalal@gmail.com"
        }
    ],
    "extra" : {
        "laravel" : {
            "providers" : [
                "Hemend\\Library\\Laravel\\Providers\\LibraryServiceProvider"
            ],
            "aliases" : {
                "Foundation" : "Hemend\\Library\\Laravel\\Facades\\Library"
            }
        }
    }
}
